{
    "name": "repository",
    "version": "0.0.1",
    "description": "Kanso package repository CouchApp",
    "load": "lib/app",
    "modules": "lib",
    "attachments": "static",
    "handlebars": {
        "templates": "templates"
    },
    "duality": {
        "base_template": "base.html"
    },
    "less": {
        "compress": true,
        "compile": "static/css/repository.less",
        "remove_from_attachments": true
    },
    "duality-contrib-session": {
        "signup": {
            "text": "Create account",
            "url": "/create_account/"
        }
    },
    "dependencies": {
        "modules": null,
        "properties": null,
        "attachments": null,
        "settings": null,
        "underscore": null,
        "datelib": null,
        "duality": null,
        "handlebars": null,
        "handlebars-helpers": null,
        "duality-handlebars": null,
        "couchtypes": null,
        "duality-contrib-session-hb": ">=0.0.2",
        "less-precompiler": null,
        "gravatar": null,
        "semver": null,
        "sanitize": null,
        "path": null
    }
}
